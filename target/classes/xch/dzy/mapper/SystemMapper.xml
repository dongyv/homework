<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xch.dzy.dao.SystemDao">
	<select id="findValueByNid" parameterType="string" resultType="string">
		select s.value from system s where s.nid = #{nid}
	</select>
</mapper>